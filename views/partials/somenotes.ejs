<!-- some notes -->
<div class="d-flex justify-content-center align-items-center vh-10">
    <div class="container">
        <div class="row justify-content-center g-4">

            <div class="row" id="notesContainer">
                <% notes.slice().reverse().forEach(note => { %>
                  <div class="col-md-4 mb-4 notice-item" data-category="<%= note.tag %>">
                    <div class="card h-100 shadow-sm">
                      <img src="/images/<%= note.imagePath%>" class="card-img-top" alt="<%= note.title %>" />
                      <div class="card-body d-flex flex-column justify-content-between">
                        <h5 class="card-title text-center"><%= note.title %></h5>
                        <div class="text-center mt-3">
                          <a href="<%= note.notesPagePath %>" class="btn btn-primary">
                            <i class="fa fa-download"></i> Download
                          </a>
                        </div>
                      </div>
                    </div>
                  </div>
                <% }) %>
              </div>
   
          



        </div>
    </div>
  </div>
<!-- some notes end -->
